        "scope": [
          "meta.preprocessor",
          "meta.preprocessor punctuation",
          "meta.preprocessor punctuation.parenthesis",
          "meta.preprocessor punctuation.squarebracket",
          "meta.preprocessor punctuation.curlybrace",
          "meta.preprocessor keyword"
        ],
        "settings": {
          "foreground": "#5a5f60"
        } // - 2
      },
      {
        "name": "Preprocessor (entities)",
        "scope": [
          "meta.preprocessor entity",
          "meta.preprocessor entity.name.method",
          "meta.preprocessor entity.name.function",
          "meta.preprocessor entity.name.variable",
          "meta.preprocessor variable",
          "meta.preprocessor string",
          "meta.preprocessor punctuation"
        ],
        "settings": {
          "foreground": "#336e72"
        } // - 1
      },
      {
        "name": "CSS",
        "scope": [
          "support.type.property-name.css"
        ],
        "settings": {
          "foreground": "#476079"
        } // - 1 456374
      },
      {
        "name": "CSS(Entity)",
        "scope": [
          "entity.name.tag.css",
          "entity.other.attribute-name.class.css"
        ],
        "settings": {
          "foreground": "#7E79B8"
        } // - 1
      },
      {
        "name": "CSS(Pixel Color)",
        "scope": [
          "keyword.other.unit.px.css"
        ],
        "settings": {
          "foreground": "#734CCA"
        } // - 1
      },
      {
        "name": "CSS(Keyword Percentage Color)",
        "scope": [
          "keyword.other.unit.percentage.css",
          "keyword.other.unit.s.css",
          "keyword.other.unit.deg.css"
        ],
        "settings": {
          "foreground": "#734CCA"
        } // - 1
      },
      {
        "name": "CSS(Property Value)",
        "scope": [
          "support.constant.property-value.css"
        ],
        "settings": {
          "foreground": "#3F7398"
        } // - 1
      },
      {
        "name": "CSS(Support Constant)",
        "scope": [
          "support.constant.color.w3c-standard-color-name.css",
          "constant.other.color.rgb-value.hex.css"
        ],
        "settings": {
          "foreground": "#6099D7"
        } // - 1
      },
      // Variables.
      {
        "name": "Variables",
        "scope": [
          "variable.other.readwrite",
          "variable.other.object",
          "variable.other.alias",
          "entity.name.variable"
        ],
        "settings": {
          "foreground": "#577880"
        }
      },
      {
        "name": "Variables",
        "scope": [
          "entity.name.variable.field.cs"
        ],
        "settings": {
          "foreground": "#734cca"
        }
      },
      {
        "name": "Variables",
        "scope": [
          "entity.other.attribute-name.html"
        ],
        "settings": {
          "foreground": "#b07290"
        }
      },
      {
        "name": "Variables (local)",
        "scope": [
          "variable.other.object.property",
          "entity.name.variable.local",
          "keyword.other.base",
          "keyword.other.this"
        ],
        "settings": {
          "foreground": "#496776"
        } // - 1
      }
    ]
  },
